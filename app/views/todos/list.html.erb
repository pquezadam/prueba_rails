<div class="container m-5">
  <div class="row">
    <div class="col-md-6">
        <h1>INCOMPLETO</h1> 
        <% @todos.each do |t| %>
            <% if t.completed? == false %>
                <%= t.description %>
            <% end %>
        <% end %>
    </div>
    <div class="col-md-6">
        <h1>COMPLETADO</h1>
        <% @todos.each do |t| %>
            <% if t.completed? == true %>
            <%= t.description %>
            <% end %>
        <% end %>
    </div>
</div>